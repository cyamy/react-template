{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:es": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy metadata --cache-location \"./node_modules/.cache/eslint/.eslintcache\"",
    "lint:style": "stylelint \"./src/**/*.{css,js,jsx,ts,tsx}\" --cache --cacheLocation \"./node_modules/.cache/stylelint/.stylelintcache\"",
    "lint:fmt": "prettier \"./src/**/*.{json,js,jsx,ts,tsx}\" --cache --cache-strategy metadata",
    "fix": "yarn fix:style && yarn fix:es && yarn fix:fmt",
    "fix:es": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix --cache --cache-strategy metadata --cache-location \"./node_modules/.cache/eslint/.eslintcache\"",
    "fix:style": "stylelint \"./src/**/*.{css,js,jsx,ts,tsx}\" --fix --cache --cacheLocation \"./node_modules/.cache/stylelint/.stylelintcache\"",
    "fix:fmt": "prettier \"./src/**/*.{json,js,jsx,ts,tsx}\" --write --cache --cache-strategy metadata",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@emotion/babel-plugin": "11.10.2",
    "@emotion/react": "11.10.4",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "@vitejs/plugin-react": "2.1.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-testing-library": "5.7.2",
    "jsdom": "20.0.1",
    "postcss": "8.4.20",
    "postcss-syntax": "0.36.2",
    "prettier": "2.7.1",
    "stylelint": "14.13.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "28.0.0",
    "typescript": "*",
    "vite": "3.2.2",
    "vitest": "0.24.3"
  }
}
